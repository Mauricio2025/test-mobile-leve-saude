{
  "expo": {
    "name": "FeedbackHub",
    "slug": "test-mobile-leve-saude",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "entryPoint": "src/App.tsx",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCF9EC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seuusuario.testmobilelevesaude"
    },
    "android": {
      "package": "com.seuusuario.testmobilelevesaude",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "c2c8d9a1-a3dc-4288-809c-6a41c02de5b3"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
